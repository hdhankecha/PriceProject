{
  "builds": [
    {
      "src": "PriceProject/wsgi.py",
      "use": "@ardnt/vercel-python-wsgi",
      "config": {
        "maxLambdaSize": "15mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "PriceProject/wsgi.py"
    }
  ]
}
{
  "env": {
    "PYTHON_VERSION": "3.10"
  }
}
